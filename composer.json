{
    "name": "clawrock/magento2-sass-preprocessor",
    "description": "Module for Sass processing during static content deployment",
    "require": {
        "magento/framework": "~101.0|~102.0|~103.0",
        "leafo/scssphp": "^0.7.7",
        "magento/module-developer": "^100.2",
        "magento/module-store": "^100.2|^101.0"
    },
    "type": "magento2-module",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "ClawRock\\SassPreprocessor\\": ""
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.3",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^3.0",
        "php-coveralls/php-coveralls": "^2.1"
    }
}
